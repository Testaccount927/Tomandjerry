local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Hydro8", "DarkTheme")
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Main")
Section:NewButton("simple spy", "ButtonInfo", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/78n/SimpleSpy/main/SimpleSpySource.lua"))()
    print("Clicked")
end)
Section:NewButton("Free hacker for break in 2", "ButtonInfo", function()
local args = {
    [1] = "Phone",
    [2] = false,
    [3] = false
}

game:GetService("ReplicatedStorage").RemoteEvents.OutsideRole:FireServer(unpack(args))

    print("Clicked")
end)
Section:NewButton("free nerd break in 2", "ButtonInfo", function()
local args = {
    [1] = "Book",
    [2] = true,
    [3] = false
}

game:GetService("ReplicatedStorage").RemoteEvents.OutsideRole:FireServer(unpack(args))

    print("Clicked")
end)
Section:NewButton("Shoot SCP 096 SCP games and monsters mini gun", "ButtonInfo", function()
local args = {
    [1] = workspace:WaitForChild("SCP 096SL"):WaitForChild("SCP 096SL"):WaitForChild("Humanoid"),
    [2] = "Minigun",
    [3] = 571533
}

game:GetService("Players").LocalPlayer.Character.Minigun.Event:FireServer(unpack(args))

    print("Clicked")
end)
Section:NewButton("clockwork thing morph scp games and Monsters", "ButtonInfo", function()
local args = {
    [1] = 4
}

game:GetService("ReplicatedStorage"):WaitForChild("MorphEvent"):FireServer(unpack(args))

    print("Clicked")
end)
